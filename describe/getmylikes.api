syntax = "v2"
info(
    title: "getmylikes"
    desc: "get my liked article"
    author: "Sean"
    email: "marttin666666@gmail.com"
    version: 1.0
)
type(
    getmylikesresp struct{
        Status int `json:"status"`
        Datalikes []Ar `json:"Datalikes"`
    }
    Ar struct{
            articaName string `json:"articleName"`
            articalLink string `json:"articleLink"`
            articalImgLink string `json:"articleImgLink"`
            articalID string `json:"articleID"`
        }
)
@server(
    group : getmylikes
    jwt:Auth
)
service yourbackend-api {
    @handler getmylikes
    get /user/myLikes returns(getmylikesresp)
}

